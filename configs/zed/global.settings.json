        "--annotation",
        "scope:global",
        "--rm",
        "mcp/aws-terraform"
      ]
    },
    "aws-api": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--name",
        "mcp-aws-api",
        "--annotation",
        "scope:global",
        "--rm",
        "mcp/aws-api-mcp-server"
      ]
    },
    "awslabs-cloudtrail": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--name",
        "mcp-aws-cloudtrail",
        "--annotation",
        "scope:global",
        "--rm",
        "mcp/cloudtrail-mcp-server"
      ]
    }
  }
}
